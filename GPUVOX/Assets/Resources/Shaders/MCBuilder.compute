float MultiSample( float3 pPosition ){
	float dist = 0.5;
	float sum = Sample3d(pPosition);
	sum += Sample3d( pPosition + float3(1,1,1) * dist);
	sum += Sample3d( pPosition + float3(-1,1,1) * dist);
	sum += Sample3d( pPosition + float3(-1,-1,1) * dist);
	sum += Sample3d( pPosition + float3(-1,-1,-1) * dist);
	sum += Sample3d( pPosition + float3(-1,-1,1) * dist);
	sum += Sample3d( pPosition + float3(-1,1,1) * dist);
	sum += Sample3d( pPosition + float3(-1,1,-1) * dist);
	sum += Sample3d( pPosition + float3(1,-1,1) * dist);
	
	return sum / 9;
	//return sum;
}

float SampleDensity( float3 pPos  ){
	
		return Sample3d(pPos);
		//return MultiSample(pPos);
}

float3 SampleNormal (float3 p)
{
	float dx = Sample3d(float3(p.x+1,p.y,p.z)) - Sample3d(float3(p.x-1,p.y,p.z));
	float dy = Sample3d(float3(p.x,p.y+1,p.z)) - Sample3d(float3(p.x,p.y-1,p.z));
	float dz = Sample3d(float3(p.x,p.y,p.z+1)) - Sample3d(float3(p.x,p.y,p.z-1));
	
	float3 normal = normalize(float3(dx,dy,dz)); //0.001+
	
	return normal;
	
}

static const int3 edge_to_neighbor[12] = {
	int3(0,0,0), //0
	int3(0,1,0), //1
	int3(1,0,0), //2
	int3(0,0,0), //3
	int3(0,0,1), //4
	int3(0,1,1), //5
	int3(1,0,1), //6
	int3(0,0,1), //7
	int3(0,0,0), //8
	int3(0,1,0), //9
	int3(1,1,0), //10
	int3(1,0,0)  //11
};
	
static const int edge_to_num[12] = {
	1, //0
	3, //1
	1, //2
	3, //3
	1, //4
	3, //5
	1, //6
	3, //7
	2, //8
	2, //9
	2, //10
	2  //11
};

int3 GetEdgeIndex3(int3 cell, int edge_index)
{
	cell+= edge_to_neighbor[edge_index];
	
	int edge_num = edge_to_num[edge_index]-1; // 0->1 8->2 3->3
	
	uint3 dim;
	_ivolume.GetDimensions(dim.x,dim.y,dim.z);
	
	return int3(cell.x,cell.y,cell.z+edge_num*32);
}

int GetEdgeIndex(int3 cell, int edge_index)
{
	cell+= edge_to_neighbor[edge_index];
	
	int edge_num = edge_to_num[edge_index]; // 0->1 8->2 3->3
	
	return ( (cell.x & 0x1F) << 0) | ( (cell.y & 0x1F) << 5) | ( (cell.z & 0x1F) << 10) | (edge_num << 15);
}

[numthreads(30,30,1)]
void BuildVertices (uint3 id : SV_DispatchThreadID)
{	
	int Index;
	int3 EdgeIndex3;
		
	float3 p;
	
	int x = id.x+1;
	int y = id.y+1;
	//int z = id.z;
	
	//Due to GPU limitations we have to use multiple passes on every thread to build a volume
	for (int qz = 1; qz < _Size+1; qz++)
	{
		//Voxel position
		p = float3(x+halfSize,y+halfSize,qz+halfSize); 
        
        int3 cell = int3(x,y,qz);
		float weights[8];
				
		weights[0] = SampleDensity(p + cubeVerts[0]);
		weights[1] = SampleDensity(p + cubeVerts[1]);
		weights[2] = SampleDensity(p + cubeVerts[2]);
		weights[3] = SampleDensity(p + cubeVerts[3]);
		weights[4] = SampleDensity(p + cubeVerts[4]);
		weights[5] = SampleDensity(p + cubeVerts[5]);
		weights[6] = SampleDensity(p + cubeVerts[6]);
		weights[7] = SampleDensity(p + cubeVerts[7]);
	
		int marchingCase = 
		(weights[7] > cap) * 128 + 
		(weights[6] > cap) * 64 +
		(weights[5] > cap) * 32 +
		(weights[4] > cap) * 16 +
		(weights[3] > cap) * 8 +
		(weights[2] > cap) * 4 +
		(weights[1] > cap) * 2 +
		(weights[0] > cap) * 1;
		
		int numpolys = case_to_numpolys[marchingCase];

		//vertex_buffer[id.x] = float3(marchingCase,case_to_numpolys[24],weights[2] );
		//vertex_buffer[id.x] = float3(weights[0],weights[1],weights[2]);//V;
		
		for( int i = 0; i < numpolys; i++ )
		{            
			//index_volume[id+int3(0,0,qz)] = 1;
			//vertex_buffer[id.x+id.y*32+qz*32*32] = float3(1,1,1);
	
            int4 polyEdges = edge_connect_list[marchingCase * 5 + i];

            //if 1st, 2nd or 3rd vertex is on the 0,3 or 8 edge 
            //calculate it and save its index in the vertex pool to index_volume                
            
            int va,vb;
            float amount;
            float3 V,N,edgePos;

			float dx,dy,dz;
			
            //VERTEX A
			if (   polyEdges.x == 0 || polyEdges.x == 8 || polyEdges.x == 3 	){
                va = edge_to_verts[polyEdges.x].x;
                vb = edge_to_verts[polyEdges.x].y;
                amount = (cap - weights[va]) / (weights[vb] - weights[va]);
                V = lerp( p + cubeVerts[va],  p + cubeVerts[vb], amount);
                
                dx = (weights[0]+weights[1]+weights[2]+weights[3])/4 - (weights[4]+weights[5]+weights[6]+weights[7])/4;
				dy = (weights[1]+weights[2]+weights[5]+weights[6])/4 - (weights[0]+weights[3]+weights[4]+weights[7])/4;
				dz = (weights[0]+weights[1]+weights[4]+weights[5])/4 - (weights[2]+weights[3]+weights[6]+weights[7])/4;
				
                //float3 d = V-p;
				
                N = normalize(float3(dx,dy,dz) );
                //N = TriLinearInterpNormal(V);

				//N = float3( (weights[vb]-(weights[va]+(weights[vb] - weights[va])/amount), dy, dz) );

                EdgeIndex3 = GetEdgeIndex3(cell, polyEdges.x);                    
                
                InterlockedAdd(index_counter,1,Index);
                
                vertex_buffer[Index] = V; //float3(weights[0],weights[1],weights[2]);
                
                vertex_buffer[Index+32000] = -N;
                
                index_volume[EdgeIndex3] = Index;
                
                count_buffer[0] = Index;
            }
            
			//VERTEX B
            if (    polyEdges.y == 0 || polyEdges.y == 8 || polyEdges.y == 3	){
                va = edge_to_verts[polyEdges.y].x;
                vb = edge_to_verts[polyEdges.y].y;
                amount = (cap - weights[va]) / (weights[vb] - weights[va]);
                V = lerp( p + cubeVerts[va],  p + cubeVerts[vb], amount);
                
                dx = (weights[0]+weights[1]+weights[2]+weights[3])/4 - (weights[4]+weights[5]+weights[6]+weights[7])/4;
				dy = (weights[1]+weights[2]+weights[5]+weights[6])/4 - (weights[0]+weights[3]+weights[4]+weights[7])/4;
				dz = (weights[0]+weights[1]+weights[4]+weights[5])/4 - (weights[2]+weights[3]+weights[6]+weights[7])/4;
				
				//float3 d = V-p;//float3(0,0,0);
				
                N = normalize(float3(dx,dy,dz) );
                //N = TriLinearInterpNormal(V);

                EdgeIndex3 = GetEdgeIndex3(cell, polyEdges.y);
                                   
                InterlockedAdd(index_counter,1,Index);
                
                vertex_buffer[Index] = V; //float3(weights[3],weights[4],weights[5]);//
                vertex_buffer[Index+32000] = -N;
                
                index_volume[EdgeIndex3] = Index;
                
                count_buffer[0] = Index;
            }
            
			//VERTEX C
            if (    polyEdges.z == 0 || polyEdges.z == 8 || polyEdges.z == 3	){
                va = edge_to_verts[polyEdges.z].x;
                vb = edge_to_verts[polyEdges.z].y;
                amount = (cap - weights[va]) / (weights[vb] - weights[va]);
                V = lerp( p + cubeVerts[va],  p + cubeVerts[vb], amount);
                
                dx = (weights[0]+weights[1]+weights[2]+weights[3])/4 - (weights[4]+weights[5]+weights[6]+weights[7])/4;
				dy = (weights[1]+weights[2]+weights[5]+weights[6])/4 - (weights[0]+weights[3]+weights[4]+weights[7])/4;
				dz = (weights[0]+weights[1]+weights[4]+weights[5])/4 - (weights[2]+weights[3]+weights[6]+weights[7])/4;
				
                //float3 d = V-p;//float3(0,0,0);
				
				N = normalize(float3(dx,dy,dz) );
                //N = TriLinearInterpNormal(V);//N = normalize(float3((dx+(d.x/2)*dx),(dy+(d.y/2)*dy),((dz+d.z/2)*dz)));

                EdgeIndex3 = GetEdgeIndex3(cell, polyEdges.z);
                                    
                InterlockedAdd(index_counter,1,Index);
                
                vertex_buffer[Index] = V; //float3(weights[6],weights[7],marchingCase);//
                vertex_buffer[Index+32000] = -N;
                
                index_volume[EdgeIndex3] = Index;
                
                count_buffer[0] = Index;
            }
           
           //index_pool[1]=Index;
       		//count_buffer[0] = Index;
		}
	}
	
}

