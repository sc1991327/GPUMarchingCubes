		weights[2] = SampleDensity(p + cubeVerts[2]);
		weights[3] = SampleDensity(p + cubeVerts[3]);
		weights[4] = SampleDensity(p + cubeVerts[4]);
		weights[5] = SampleDensity(p + cubeVerts[5]);
		weights[6] = SampleDensity(p + cubeVerts[6]);
		weights[7] = SampleDensity(p + cubeVerts[7]);
	
		int marchingCase = 
		(weights[7] > cap) * 128 + 
		(weights[6] > cap) * 64 +
		(weights[5] > cap) * 32 +
		(weights[4] > cap) * 16 +
		(weights[3] > cap) * 8 +
		(weights[2] > cap) * 4 +
		(weights[1] > cap) * 2 +
		(weights[0] > cap) * 1;
		
		int numpolys = case_to_numpolys[marchingCase];

		for( int i = 0; i < numpolys; i++ )
		{
		
        	//if (cell.x >= _Size-1 || cell.y >= _Size-1 || cell.z >= _Size-1)
        	//	return;
        
           	int4 polyEdges = edge_connect_list[marchingCase * 5 + i];

            Tris T;

            T.A = index_volume[ GetEdgeIndex3(cell, polyEdges.x) ];
            T.B = index_volume[ GetEdgeIndex3(cell, polyEdges.y) ];
            T.C = index_volume[ GetEdgeIndex3(cell, polyEdges.z) ];
            
            //half3 NA = vertex_buffer[T.A+32000]; //half3(0,0,0);//-TriLinearInterpNormal(P.A);
            //half3 NB = vertex_buffer[T.B+32000]; //half3(0,0,0);//-TriLinearInterpNormal(P.B);
            //half3 NC = vertex_buffer[T.C+32000]; //half3(0,0,0);//-TriLinearInterpNormal(P.C);
                        
        	InterlockedAdd(index_counter,1,Index);
        	triangles_buffer[Index] = T;
        	
        	count_buffer[1] = Index;
		}
	}
}
	